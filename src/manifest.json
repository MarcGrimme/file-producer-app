{
  "name": "Files Producer App",
  "version": "1.1",
  "manifest_version": 2,
  "minimum_chrome_version": "23",
  "app": {
    "background": {
      "scripts": ["background.js"]
    }
  },
  "permissions": [
    "unlimitedStorage",
    "storage",
    { "fileSystem": [ "write", "retainEntries", "directory" ] }
  ]
}
